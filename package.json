{
  "name": "svelte-preprocess-filter",
  "version": "1.0.0",
  "description": "Filter utility for svelte preprocessors",
  "main": "out/index.js",
  "module": "out/module.js",
  "scripts": {
    "compile": "rollup -c",
    "lint": "eslint rollup.config.js src",
    "test": "nyc ava"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ls-age/svelte-preprocess-filter.git"
  },
  "keywords": [
    "svelte",
    "preprocess",
    "utility",
    "filter"
  ],
  "author": "Lukas Hechenberger <npm@ls-age.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ls-age/svelte-preprocess-filter/issues"
  },
  "homepage": "https://github.com/ls-age/svelte-preprocess-filter#readme",
  "devDependencies": {
    "@ls-age/babel-preset": "0.4.0",
    "@ls-age/bump-version": "0.2.0",
    "@ls-age/eslint-config": "0.9.0",
    "ava": "2.1.0",
    "babel-core": "6.26.3",
    "codecov": "3.5.0",
    "eslint": "5.16.0",
    "nyc": "14.1.1",
    "rollup": "1.14.6",
    "rollup-plugin-babel": "4.3.2",
    "tap-xunit": "2.4.1"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 2017
    },
    "extends": "@ls-age",
    "settings": {
      "import/core-modules": [
        "less"
      ]
    }
  },
  "babel": {
    "presets": [
      [
        "@ls-age/babel-preset",
        {
          "modules": false
        }
      ]
    ],
    "env": {
      "test": {
        "presets": [
          "@ls-age/babel-preset"
        ],
        "sourceMaps": "inline"
      }
    }
  },
  "ava": {
    "require": [
      "babel-core/register"
    ]
  },
  "renovate": {
    "extends": [
      "@ls-age"
    ],
    "devDependencies": {
      "automerge": true
    }
  }
}
